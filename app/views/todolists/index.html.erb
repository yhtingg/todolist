<h1>My Todo List!</h1>

<%= link_to 'New Todo Item', new_todolist_path %>

<table>
  <tr>
    <th>Todo</th>
    <th></th>
  </tr>

  <% @todolists.each do |todolist| %>
    <tr>
      <td><%= todolist.todo %></td>
      <td><%= link_to 'Show', todolist_path(todolist) %></td>
    </tr>
  <% end %>
</table>
